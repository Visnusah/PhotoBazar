{
  "name": "photobazaar-backend",
  "version": "1.0.0",
  "description": "Backend API for PhotoBazaar - A photo marketplace application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server-simple.js",
    "dev": "nodemon server-simple.js",
    "start:old": "node server.js",
    "dev:old": "nodemon server.js",
    "db:init": "node scripts/init-db.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@react-email/components": "^0.1.1",
    "@react-email/render": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "react-email": "^4.0.17",
    "sequelize": "^6.35.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": [
    "photography",
    "marketplace",
    "api",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "PhotoBazaar Team",
  "license": "MIT"
}
